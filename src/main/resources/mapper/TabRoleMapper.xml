<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0 //EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lsl.exam.mapper.TabRoleMapper">
    <select id="qryRoleInfoByUserId" resultType="java.util.Map" parameterType="java.util.Map">
        select t1.id,t1.role,t1.remark from tab_role t1
        join tab_userrole t2 on t1.id=t2.roleid
        where t2.userid = #{userId}

    </select>
</mapper>